                </listitem>
                <listitem>
                    <para>
                        <code>tenant_id</code>
                    </para>
                </listitem>
                <listitem>
                    <para>
                        <code>name</code></para>
                </listitem>
                <listitem>
                    <para>
                        <code>description</code>
                    </para>
                </listitem>
                <listitem>
                    <para>
                        <code>vip_subnet_id</code>
                    </para>
                </listitem>
                <listitem>
                    <para>
                        <code>vip_address</code>
                    </para>
                </listitem>
                <listitem>
                    <para>
                        <code>admin_state_up</code>
                    </para>
                </listitem>
                <listitem>
                    <para>
                        <code>listeners</code>
                    </para>
                </listitem>
                <listitem>
                    <para>
                        <code>provisioning_status</code>
