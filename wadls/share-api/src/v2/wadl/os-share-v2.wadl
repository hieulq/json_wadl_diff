<!DOCTYPE application[
   <!ENTITY % common  SYSTEM "../common.ent">
   %common;
   <!ENTITY % common_project  SYSTEM "../../../../common_project.ent">
   %common_project;
   ]>
<application xmlns="http://wadl.dev.java.net/2009/02"
  xmlns:common="http://docs.openstack.org/common/api/v1.0"
  xmlns:csapi="http://docs.openstack.org/share/api/v1.0"
  xmlns:db="http://docbook.org/ns/docbook"
  xmlns:wadl="http://wadl.dev.java.net/2009/02"
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns:xsdxt="http://docs.rackspacecloud.com/xsd-ext/v1.0">
  <resources base="https://share_server_id.example.com/" xml:id="os-share-v2.0">
    <resource id="share-versions-v2" path="/">
      <param name="X-Openstack-Manila-Api-Version" style="header" type="xsd:string">
        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
          <para>
            The HTTP header to specify a valid Shared File Systems API
            micro-version. For example, <code>"X-Openstack-Manila-Api-Version:
            2.6"</code>. If you omit this header, the default micro- version is
            2.0.
          </para>
        </wadl:doc>
      </param>
      <method href="#listVersions"/>
      <resource id="share-version" path="v2">
        <resource id="tenant_id" path="{tenant_id}">
          &tenant_idTemplateParameter;
          <resource id="extensions" path="extensions">
            <method href="#listExtensions" />
          </resource>
          <resource id="limits" path="limits">
            <method href="#listLimits" />
          </resource>
          <resource id="scheduler-stats" path="scheduler-stats">
            <resource id="pools" path="pools">
              <method href="#listPools" />
              <resource id="detail-pools" path="detail">
                <method href="#listPoolsDetails" />
              </resource>
            </resource>
          </resource>
          <resource id="shares" path="shares">
            <method href="#createShare" />
            <method href="#listShares" />
            <resource path="detail" id="detail-share">
              <method href="#listSharesDetails" />
            </resource>
            <resource path="{share_id}" id="share_id">
              &share_idTemplateParameter;
              <method href="#showShare" />
              <method href="#updateShare" />
              <method href="#deleteShare" />
              <resource id="action" path="action">
                <method href="#grantAccess" />
                <method href="#revokeAccess" />
                <method href="#listAccessRules" />
                <method href="#resetShareState" />
                <method href="#force-deleteShare" />
                <method href="#extendShare" />
                <method href="#shrinkShare" />
              </resource>
              <resource path="metadata" id="metadata-share">
                <method href="#showShareMetadata" />
                <method href="#updateShareMetadata" />
                <method href="#setShareMetadata" />
                <resource path="{key}" id="metadata_key">
                  &metadata_keyTemplateParameter;
                  <method href="#unSetShareMetadata" />
                </resource>
              </resource>
            </resource>
          </resource>
          <resource id="snapshots" path="snapshots">
            <method href="#share-createSnapshot" />
            <method href="#share-listSnapshots" />
            <resource path="detail" id="detail-snapshot">
              <method href="#share-listSnapshotsDetails" />
            </resource>
            <resource path="{snapshot_id}" id="snapshot_id">
              &snapshot_idTemplateParameter;
              <method href="#share-showSnapshot" />
              <method href="#share-updateSnapshot" />
              <method href="#share-deleteSnapshot" />
              <resource id="snapshot-action" path="action">
                <method href="#resetSnapshotState" />
                <method href="#forceshare-deleteSnapshot" />
              </resource>
            </resource>
          </resource>
          <resource id="share-networks" path="share-networks">
            <method href="#createShareNetwork" />
            <method href="#listShareNetworks" />
            <resource path="detail" id="detail-share-network">
              <method href="#listShareNetworksDetails" />
            </resource>
            <resource path="{share_network_id}" id="share_network_id">
              &share_network_idTemplateParameter;
              <method href="#showShareNetwork" />
              <method href="#updateShareNetwork" />
              <method href="#deleteShareNetwork" />
              <resource id="share-network-action" path="action">
                <method href="#addSecurityService" />
                <method href="#removeSecurityService" />
              </resource>
            </resource>
          </resource>
          <resource id="security-services" path="security-services">
            <method href="#createSecurityService" />
            <method href="#listSecurityServices" />
            <resource path="detail" id="detail-security-service">
              <method href="#listSecurityServicesDetails" />
            </resource>
            <resource path="{security_service_id}" id="security_service_id">
              &security_service_idTemplateParameter;
              <method href="#showSecurityService" />
              <method href="#updateSecurityService" />
              <method href="#deleteSecurityService" />
            </resource>
          </resource>
          <resource id="share-servers-v2" path="share-servers">
            <method href="#listShareServers" />
            <resource path="{share_server_id}" id="share_server_id">
              &share_server_idTemplateParameter;
              <method href="#showShareServer" />
              <method href="#deleteShareServer" />
              <resource path="detail" id="share_server_detail">
                  <method href="#showShareServerDetails" />
              </resource>
            </resource>
          </resource>
          <resource id="share-type" path="types">
            <method href="#share-createShareType" />
            <method href="#share-listShareTypes" />
            <resource path="default" id="default-share-type">
              <method href="#listDefaultShareTypes" />
            </resource>
            <resource path="{share_type_id}" id="share_type_id">
              &share_type_idTemplateParameter;
              <method href="#share-deleteShareType" />
              <resource path="extra_specs" id="extra-specs-share-type">
                <method href="#listExtraSpecsShareType" />
                <method href="#setExtraSpecShareType" />
                <resource path="{key}" id="extra-spec-key">
                  &extra_spec_keyTemplateParameter;
                  <method href="#unsetExtraSpecShareType" />
                </resource>
              </resource>
              <resource id="share-type-action" path="action">
                <method href="#addShareTypeAccess" />
                <method href="#removeShareTypeAccess" />
              </resource>
              <resource id="os-share-type-access" path="os-share-type-access">
                <method href="#showShareTypeAccess" />
              </resource>
            </resource>
          </resource>
          <resource id="share-instances" path="share_instances">
            <method href="#listShareInstances" />
            <resource path="{share_instance_id}" id="share_instance_id">
              &share_instance_idTemplateParameter;
              <method href="#showShareInstance" />
              <resource id="share-instances-action" path="action">
                <method href="#resetShareInstanceState" />
                <method href="#force-deleteShareInstance" />
              </resource>
            </resource>
          </resource>
          <resource id="os-services" path="os-services">
            <method href="#listServices" />
            <resource id="enable-service" path="enable">
              <method href="#enableService" />
            </resource>
            <resource id="disable-service" path="disable">
              <method href="#disableService" />
            </resource>
          </resource>
          <resource id="os-share-manage" path="os-share-manage">
            <method href="#manageShare" />
          </resource>
          <resource id="os-share-unmanage" path="os-share-unmanage">
            <resource id="os-share-unmanage-share_id" path="{share_id}">
              &share_idTemplateParameter;
              <resource id="unmanage" path="unmanage">
                <method href="#unmanageShare" />
              </resource>
            </resource>
          </resource>
          <resource id="os-quota-sets" path="os-quota-sets">
            <resource id="tenant_id-os-quota-sets" path="{tenant_id}" >
              &tenant_idTemplateParameter2;
              <param name="user_id" style="query"
                type="csapi:UUID" required="false">
                <wadl:doc
                  xmlns="http://docbook.org/ns/docbook"
                  xml:lang="EN" title="User UUID">
                  <para>
                    The UUID of the user. If you specify this query parameter,
                    you update the quotas for this user in the tenant. If you
                    omit this parameter, you update the quotas for the
                    project.
                  </para>
                </wadl:doc>
              </param>
              <method href="#os-quota-sets-showQuota"/>
              <method href="#os-quota-sets-updateQuota"/>
              <method href="#os-quota-sets-deleteQuota"/>
              <resource path="defaults" id="os-quota-sets-defaults">
                <method href="#os-quota-sets-listDefaultQuotas"/>
              </resource>
            </resource>
          </resource>
          <resource id="os-availability-zone" path="os-availability-zone">
            <method href="#listAvailabilityZones" />
          </resource>
        </resource>
      </resource>
    </resource>
  </resources>
   <!-- METHODS -->
  <method name="GET" id="listVersions">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List versions">
      <para role="shortdesc">
        Lists all Shared File Systems API versions.
      </para>
    </wadl:doc>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-versions-response.json" />
          </wadl:doc>
       </representation>
    </response>
  </method>
  <method name="POST" id="createShare">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Create share">
      <para role="shortdesc">
        Creates a share.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-create-request.json" />
          </wadl:doc>
          &createShareRequestParameters;
       </representation>
    </request>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-create-response.json" />
          </wadl:doc>
          &createShareResponseParameters;
       </representation>
    </response>
  </method>
  <method name="GET" id="listShares">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List shares">
      <para role="shortdesc">
        Lists all shares.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          &listSharesRequestParameters;
       </representation>
    </request>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-shares-list-response.json" />
          </wadl:doc>
          &listSharesResponseParameters;
       </representation>
    </response>
  </method>
  <method name="GET" id="listSharesDetails">
    <wadl:doc xmlns="http://docbook.org/ns/docbook"
      xml:lang="EN" title="List shares with details">
      <para role="shortdesc">
        Lists all shares, with details.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          &listSharesRequestParameters;
       </representation>
    </request>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-shares-list-detailed-response.json" />
          </wadl:doc>
          &listSharesDetailsResponseParameters;
       </representation>
    </response>
  </method>
  <method name="GET" id="showShare">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Show share details">
      <para role="shortdesc">
        Shows details for a share.
      </para>
    </wadl:doc>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-show-response.json" />
          </wadl:doc>
          &listSharesDetailsResponseParameters;
       </representation>
    </response>
  </method>
  <method name="PUT" id="updateShare">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Update share">
      <para role="shortdesc">
        Updates a share.
      </para>
      <para>
        You can update these attributes:
      </para>
      <itemizedlist>
        <listitem>
          <para>
            <code>display_name</code>, which also changes the
            <code>name</code> of the share.
          </para>
        </listitem>
        <listitem>
          <para>
            <code>display_description</code>, which also changes the
            <code>description</code> of the share.
         </para>
        </listitem>
        <listitem>
          <para>
            <code>is_public</code>. Changes the level of visibility.
          </para>
        </listitem>
      </itemizedlist>
      <para>
        If you try to update other attributes, they retain their
        previous values.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-update-request.json" />
          </wadl:doc>
          &updateShareRequestParameters;
       </representation>
    </request>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-update-response.json" />
          </wadl:doc>
          &listSharesDetailsResponseParameters;
       </representation>
    </response>
  </method>
  <method name="DELETE" id="deleteShare">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Delete share">
      <para role="shortdesc">
        Deletes a share.
      </para>
    </wadl:doc>
    <request>
      <representation mediaType="application/json">
        &deleteShareRequestParameter;
      </representation>
    </request>
    <response status="202" />
  </method>
  <method name="GET" id="showShareMetadata">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Show share metadata">
      <para role="shortdesc">
        Shows the metadata for a share.
      </para>
    </wadl:doc>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-show-metadata-response.json" />
          </wadl:doc>
          &shareMetadataObject;
       </representation>
    </response>
  </method>
  <method name="PUT" id="updateShareMetadata">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Update share metadata">
      <para role="shortdesc">
        Updates the metadata for a share.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-update-metadata-request.json" />
          </wadl:doc>
          &shareMetadataRequestParameter;
       </representation>
    </request>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-update-metadata-response.json" />
          </wadl:doc>
          &shareMetadataObject;
       </representation>
    </response>
  </method>
  <method name="POST" id="setShareMetadata">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Set share metadata">
      <para role="shortdesc">
        Sets the metadata on a share.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-set-metadata-request.json" />
          </wadl:doc>
          &shareMetadataRequestParameter;
       </representation>
    </request>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-set-metadata-response.json" />
          </wadl:doc>
          &shareMetadataObject;
       </representation>
    </response>
  </method>
  <method name="DELETE" id="unSetShareMetadata">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Unset share metadata">
      <para role="shortdesc">
        Unsets the metadata on a share.
      </para>
      <para>
        To unset a metadata key value, specify only the key name in the URI.
      </para>
    </wadl:doc>
    <response status="200" />
  </method>
  <method name="POST" id="grantAccess">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Grant access">
      <para role="shortdesc">
        Grants access to a share.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-actions-grant-access-request.json" />
          </wadl:doc>
          &grantAccessRequestParameters;
       </representation>
    </request>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-actions-grant-access-response.json" />
          </wadl:doc>
          &grantAccessResponseParameters;
       </representation>
    </response>
  </method>
  <method name="POST" id="revokeAccess">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Revoke access">
      <para role="shortdesc">
        Revokes access from a share.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-actions-revoke-access-request.json" />
          </wadl:doc>
          &revokeAccessRequestParameters;
       </representation>
    </request>
    <response status="202" />
  </method>
  <method name="POST" id="listAccessRules">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List access rules">
      <para role="shortdesc">
        Lists access rules for a share.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-actions-list-access-rules-request.json" />
          </wadl:doc>
          &listAccessRulesRequestParameters;
       </representation>
    </request>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-actions-list-access-rules-response.json" />
          </wadl:doc>
          &listAccessRulesResponseParameters;
       </representation>
    </response>
  </method>
  <method name="POST" id="resetShareState">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Reset share state">
      <para role="shortdesc">
        Administrator only. Explicitly updates the state of a share.
      </para>
      <para>
        Use the <code>policy.json</code> file to grant permissions for this
        action to other roles.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-actions-reset-state-request.json" />
          </wadl:doc>
          &resetShareStateRequestParameter;
       </representation>
    </request>
    <response status="202" />
  </method>
  <method name="POST" id="force-deleteShare">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Force-delete share">
      <para role="shortdesc">
        Administrator only. Force-deletes a share in any state.
      </para>
      <para>
        Use the <code>policy.json</code> file to grant permissions for this
        action to other roles.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-actions-force-delete-request.json" />
          </wadl:doc>
          &force-deleteShareRequestParameter;
       </representation>
    </request>
    <response status="202" />
  </method>
  <method name="POST" id="extendShare">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Extend share">
      <para role="shortdesc">
        Increases the size of a share.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-actions-extend-request.json" />
          </wadl:doc>
          &extendShareRequestParameters;
       </representation>
    </request>
    <response status="202" />
  </method>
  <method name="POST" id="shrinkShare">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Shrink share">
      <para role="shortdesc">
        Shrinks the size of a share.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-share-actions-shrink-request.json" />
          </wadl:doc>
          &shrinkShareRequestParameters;
       </representation>
    </request>
    <response status="202" />
  </method>
  <method name="POST" id="share-createSnapshot">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Create share snapshot">
       <para role="shortdesc">
        Creates a snapshot from a share.
      </para>
    </wadl:doc>
    <request>
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-snapshot-create-request.json" />
          </wadl:doc>
          &share-createSnapshotRequestParameters;
       </representation>
    </request>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-snapshot-create-response.json" />
          </wadl:doc>
          &share-createSnapshotResponseParameters;
       </representation>
    </response>
  </method>
  <method name="GET" id="share-listSnapshots">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List share snapshots">
       <para role="shortdesc">
        Lists all share snapshots.
      </para>
    </wadl:doc>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-snapshots-list-response.json" />
          </wadl:doc>
          &share-getSnapshotsResponseParameters;
       </representation>
    </response>
  </method>
  <method name="GET" id="share-listSnapshotsDetails">
    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List share snapshots with details">
       <para role="shortdesc">
        Lists all share snapshots with details.
      </para>
    </wadl:doc>
    <response status="200">
       <representation mediaType="application/json">
          <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
             <xsdxt:code href="../samples/manila-snapshots-list-detailed-response.json" />
          </wadl:doc>
          &share-getSnapshotResponseParameters;
       </representation>
    </response>
  </method>
  <method name="GET" id="share-showSnapshot">
